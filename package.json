{
  "dependencies": {
    "@badeball/cypress-cucumber-preprocessor": "^15.1.1",
    "@bahmutov/cypress-esbuild-preprocessor": "^2.1.5",
    "cypress": "^12.3.0"
  },
  "devDependencies": {
    "cypress-recurse": "^1.27.0"
  },
  "cypress-cucumber-preprocessor": {
    "filterSpecs": true,
    "omitFiltered": true,
    "cucumberJson": {
      "generate": true,
      "outputFolder": "cypress/report/cucumber-json",
      "filePrefix": "",
      "fileSuffix": ".cucumber"
    },
    "stepDefinitions": [
      "[filepath]/**/*.{js,ts}",
      "[filepath].{js,ts}",
      "cypress/e2e/step_definitions/*.{js,ts}",
      "[filepath]\\***.{js,ts}",
      "[filepath].{js,ts}",
      "cypress\\e2e\\step_definitions\\*.{js,ts}"
    ]
  }
}
